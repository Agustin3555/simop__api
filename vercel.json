{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "*.ts": {
      "includeFiles": ["node_modules/.prisma/**/", "prisma/schema.prisma"]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "HEAD", "DELETE", "OPTIONS"]
    }
  ]
}
